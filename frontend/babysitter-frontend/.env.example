# ============================================================================
# VARIABLES DE ENTORNO PARA FRONTEND REACT
# ============================================================================
# Copia este archivo a .env para desarrollo local
# Las variables deben comenzar con REACT_APP_

# URL base de la API del backend
REACT_APP_API_URL=http://localhost:5000/api

# Modo de desarrollo (development/production)
REACT_APP_ENV=development

# Habilitar logs de debug
REACT_APP_DEBUG=true

# Configuración para producción (se configura automáticamente en Cloud Run)
# REACT_APP_API_URL=https://backend-url-from-cloud-run.com/api
# REACT_APP_ENV=production
# REACT_APP_DEBUG=false